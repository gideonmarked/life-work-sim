; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Live Laugh Level"
run/main_scene="res://scenes/screens/MainMenu.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

DebugProfiler="*res://scripts/autoloads/DebugProfiler.gd"
Balance="*res://scripts/autoloads/Balance.gd"
TimeManager="*res://scripts/autoloads/TimeManager.gd"
ConsoleLog="*res://scripts/autoloads/ConsoleLog.gd"
GameState="*res://scripts/autoloads/GameState.gd"
SaveManager="*res://scripts/autoloads/SaveManager.gd"
ActionQueue="*res://scripts/autoloads/ActionQueue.gd"
CharacterAI="*res://scripts/autoloads/CharacterAI.gd"

[display]

window/size/viewport_width=1900
window/size/viewport_height=1000
window/size/resizable=false
window/size/borderless=true
window/size/transparent=true
window/stretch/mode="canvas_items"
window/per_pixel_transparency/allowed=true
